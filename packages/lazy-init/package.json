{
   "name": "lazy-init",
   "version": "2.2.0",
   "license": "MIT",
   "description": "Lazily initialize values by deferring their creation until first use, resulting in better performance.",
   "homepage": "https://github.com/reiss-d/lazy-init#readme",
   "repository": "https://github.com/reiss-d/lazy-init.git",
   "bugs": "https://github.com/reiss-d/lazy-init/issues",
   "author": "Reiss Davidson",
   "exports": {
      "./package.json": "./package.json",
      ".": {
         "types": "./dist/index.d.ts",
         "import": "./dist/index.mjs",
         "module": "./dist/index.esm.js",
         "default": "./dist/index.js"
      },
      "./cache": {
         "import": "./dist/cache.mjs",
         "types": "./dist/cache.d.ts",
         "module": "./dist/cache.esm.js",
         "default": "./dist/cache.js"
      }
   },
   "typesVersions": {
      "*": {
         "cache": ["dist/cache.d.ts"]
      }
   },
   "main": "./dist/index.js",
   "types": "./dist/index.d.ts",
   "files": [
      "dist",
      "package.json",
      "LICENSE"
   ],
   "sideEffects": false,
   "scripts": {
      "build": "tsup-node --config ./tsup.config.ts && cp ./dist/index.d.ts ./dist/cache.d.ts && cp ../../README.md .",
      "test": "jest",
      "typedoc": "typedoc --excludeExternals"
   },
   "devDependencies": {
      "@lazy-init/swc-core-plugin": "workspace:*",
      "@swc/cli": "^0.1.62",
      "@swc/core": "1.3.40",
      "@swc/helpers": "^0.4.14",
      "@swc/jest": "^0.2.24",
      "eslint-config-internal-ts": "workspace:*",
      "eslint-plugin-lazy-init": "workspace:*",
      "tsconfig": "workspace:*",
      "tsup": "^6.6.3",
      "typescript": "^4.9.5"
   },
   "keywords": [
      "cache",
      "defer",
      "hoist",
      "initialise",
      "initialize",
      "lazy",
      "lazily",
      "static",
      "swc-plugin"
   ]
}
