{
   "name": "lazy-init",
   "version": "2.2.0",
   "license": "MIT",
   "description": "Lazily initialize values by deferring their creation until first use, resulting in better performance.",
   "homepage": "https://github.com/reiss-d/lazy-init#readme",
   "repository": "https://github.com/reiss-d/lazy-init.git",
   "bugs": "https://github.com/reiss-d/lazy-init/issues",
   "author": "Reiss Davidson",
   "exports": {
      "./package.json": "./package.json",
      ".": {
         "import": {
            "types@<5.0": "./dist/esm/index.d.mts",
            "types": "./dist/ts5.0/esm/index.d.mts",
            "default": "./dist/esm/index.mjs"
         },
         "module": {
            "types@<5.0": "./dist/esm/index.esm.d.ts",
            "types": "./dist/ts5.0/esm/index.esm.d.ts",
            "default": "./dist/esm/index.esm.js"
         },
         "require": {
            "types@<5.0": "./dist/index.d.ts",
            "types": "./dist/ts5.0/index.d.ts",
            "default": "./dist/index.js"
         }
      },
      "./cache": {
         "import": {
            "types@<5.0": "./dist/esm/cache.d.mts",
            "types": "./dist/ts5.0/esm/cache.d.mts",
            "default": "./dist/esm/cache.mjs"
         },
         "module": {
            "types@<5.0": "./dist/esm/cache.esm.d.ts",
            "types": "./dist/ts5.0/esm/cache.esm.d.ts",
            "default": "./dist/esm/cache.esm.js"
         },
         "require": {
            "types@<5.0": "./dist/cache.d.ts",
            "types": "./dist/ts5.0/cache.d.ts",
            "default": "./dist/cache.js"
         }
      }
   },
   "typesVersions": {
      "<5.0": {
         "dist/index.d.ts": ["dist/index.d.ts"],
         "cache": ["dist/cache.d.ts"],
         "*": ["dist/*"]
      },
      "*": {
         "dist/index.d.ts": ["dist/ts5.0/index.d.ts"],
         "cache": ["dist/ts5.0/cache.d.ts"],
         "*": ["dist/ts5.0/*"]
      }
   },
   "main": "./dist/index.js",
   "module": "./dist/esm/index.mjs",
   "types": "./dist/index.d.ts",
   "files": [
      "dist",
      "package.json",
      "LICENSE"
   ],
   "sideEffects": false,
   "scripts": {
      "build": "pnpm clean && tsup-node --config tsup.config.ts && pnpm downlevel",
      "clean": "shx rm -rf dist && shx cp -u ../../README.md . && shx mkdir -p dist/ts5.0/esm",
      "downlevel": "shx cp -R dist/*.d.ts dist/ts5.0 && concurrently \"node downlevel.js\" \"node downlevel.js --ts5\"",
      "test": "jest",
      "typedoc": "typedoc --excludeExternals"
   },
   "dependencies": {
      "ts-essentials": "^9.3.1",
      "tslib": "^2.5.0"
   },
   "devDependencies": {
      "@lazy-init/swc-core-plugin": "workspace:*",
      "@swc/cli": "^0.1.62",
      "@swc/core": "1.3.55",
      "@swc/helpers": "^0.5.1",
      "@swc/jest": "^0.2.26",
      "eslint-config-internal-ts": "workspace:*",
      "eslint-plugin-lazy-init": "workspace:*",
      "tsconfig": "workspace:*",
      "tsup": "^6.7.0",
      "typescript": "^5.0.4"
   },
   "keywords": [
      "cache",
      "defer",
      "hoist",
      "initialise",
      "initialize",
      "lazy",
      "lazily",
      "static",
      "swc-plugin"
   ]
}
